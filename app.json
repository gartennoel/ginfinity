{
  "expo": {
    "name": "ginfinity_cross_platform",
    "description": "",
    "slug": "ginfinity_cross_platform",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.6.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
